<?xml version="1.0" encoding="UTF-8"?><layout title="Управление услугой:" view="view">
  <AnchorsPane background="#ffffff" cursor="0" name="view" prefHeight="445px" prefWidth="405px"/>
  <TabbedPane cursor="0" name="tabbedPane" parent="view" prefHeight="445px" prefWidth="405px" selectedComponent="pnlServiseCard">
    <AnchorsPaneConstraints bottom="0px" left="0px" right="0px" top="0px"/>
  </TabbedPane>
  <AnchorsPane background="#ffffff" cursor="0" name="pnlServiseCard" parent="tabbedPane" prefHeight="415px" prefWidth="400px">
    <TabbedPaneConstraints tabIcon="icons/Settings/edit2.png" tabTitle="Карточка услуги" tabToolTip=""/>
  </AnchorsPane>
  <AnchorsPane background="#ffffff" cursor="0" name="pnlServiseWithBill" parent="tabbedPane" prefHeight="415px" prefWidth="400px" visible="false">
    <TabbedPaneConstraints tabIcon="icons/16x16/bill.png" tabTitle="Подключенные счета" tabToolTip=""/>
  </AnchorsPane>
  <ToolBar cursor="0" name="toolBar" parent="pnlServiseWithBill" prefHeight="30px" prefWidth="140px">
    <AnchorsPaneConstraints height="30px" left="0px" right="260px" top="0px"/>
  </ToolBar>
  <Button cursor="0" focusable="false" horizontalTextPosition="0" icon="icons/new_bill/new_bill.png" name="btnAddBill" parent="toolBar" prefHeight="30px" prefWidth="32px" toolTipText="Добавить услугу на счёт" verticalTextPosition="3"/>
  <Button cursor="0" focusable="false" horizontalTextPosition="0" icon="icons/Play.Pause/pause.png" name="btnPause" parent="toolBar" prefHeight="30px" prefWidth="29px" toolTipText="Приостановить услугу на счету" verticalTextPosition="3"/>
  <Button cursor="0" focusable="false" horizontalTextPosition="0" icon="icons/22x22/delete.png" name="btnDel" parent="toolBar" prefHeight="30px" prefWidth="29px" toolTipText="Безвозвратно удалить услугу со счёта" verticalTextPosition="3"/>
  <Button cursor="0" focusable="false" horizontalTextPosition="0" icon="icons/22x22/money1.png" name="btnMoney" parent="toolBar" prefHeight="30px" prefWidth="29px" toolTipText="Просмотреть остаток денежных средств на счету в рублях" verticalTextPosition="3"/>
  <Label cursor="0" horizontalAlignment="4" name="lbSearch" parent="pnlServiseWithBill" prefHeight="30px" prefWidth="67px" text="Поиск: ">
    <AnchorsPaneConstraints height="30px" left="140px" top="0px" width="67px"/>
  </Label>
  <ModelFormattedField cursor="0" format="" name="mffSearch" parent="pnlServiseWithBill" prefHeight="30px" prefWidth="185px" toolTipText="Поиск по ID">
    <AnchorsPaneConstraints height="30px" left="215px" right="0px" top="0px"/>
  </ModelFormattedField>
  <ModelGrid cursor="0" deletable="false" editable="false" insertable="false" name="mgBillsOnService" parent="pnlServiseWithBill" prefHeight="385px" prefWidth="400px">
    <AnchorsPaneConstraints bottom="0px" left="0px" right="0px" top="30px"/>
    <RadioGridColumn name="colSelected"/>
    <ModelGridColumn name="colBillId" preferredWidth="80px" title="ID счета" width="115px">
      <ModelFormattedField format=""/>
    </ModelGridColumn>
    <ModelGridColumn name="colCount" preferredWidth="65px" title="Остаток счетчика" visible="false" width="100px">
      <ModelFormattedField format=""/>
    </ModelGridColumn>
    <ModelGridColumn name="colDate" title="Дата списания" visible="false" width="111px">
      <ModelDate/>
    </ModelGridColumn>
    <ModelGridColumn maxWidth="110px" minWidth="22px" name="colPause" preferredWidth="50px" title="Приостановлен" width="50px">
      <ModelCheckBox/>
    </ModelGridColumn>
  </ModelGrid>
</layout>
