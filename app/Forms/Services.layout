<?xml version="1.0" encoding="UTF-8"?><layout title="Управление услугами" view="view">
  <AnchorsPane background="#ffffff" cursor="0" name="view" prefHeight="399px" prefWidth="533px"/>
  <ToolBar cursor="0" name="toolBar" parent="view" prefHeight="30px" prefWidth="533px">
    <AnchorsPaneConstraints height="30px" left="0px" right="0px" top="0px"/>
  </ToolBar>
  <Button cursor="0" focusable="false" horizontalTextPosition="0" icon="icons/22x22/add.png" name="btnNewService" parent="toolBar" prefHeight="30px" prefWidth="29px" toolTipText="Создать услугу" verticalTextPosition="3"/>
  <Button cursor="0" focusable="false" horizontalTextPosition="0" icon="icons/Settings/settings.png" name="btnSettings" parent="toolBar" prefHeight="30px" prefWidth="34px" toolTipText="Изменить/Подключить на счёт" verticalTextPosition="3"/>
  <Button cursor="0" focusable="false" horizontalTextPosition="0" icon="icons/22x22/active.png" name="btnActive" parent="toolBar" prefHeight="30px" prefWidth="29px" toolTipText="Активировать/деактивировать услугу" verticalTextPosition="3"/>
  <Button cursor="0" focusable="false" horizontalTextPosition="0" icon="icons/22x22/delete.png" name="btnDel" parent="toolBar" prefHeight="30px" prefWidth="29px" toolTipText="Деактивировать и отписать от всех счетов" verticalTextPosition="3"/>
  <Button cursor="0" focusable="false" horizontalTextPosition="0" icon="icons/refresh/refresh.png" name="btnRefresh" parent="toolBar" prefHeight="30px" prefWidth="29px" toolTipText="Обновить список услуг" verticalTextPosition="3"/>
  <Label cursor="0" name="label" parent="toolBar" prefHeight="30px" prefWidth="20px"/>
  <CheckBox cursor="0" focusable="false" name="cbActive" parent="toolBar" prefHeight="30px" prefWidth="100px" selected="true" text="Активные" toolTipText="Фильтр"/>
  <Label cursor="0" name="label1" parent="toolBar" prefHeight="30px" prefWidth="15px"/>
  <Label cursor="0" name="label2" parent="toolBar" prefHeight="30px" prefWidth="50px" text="Поиск:"/>
  <ModelFormattedField cursor="0" format="" name="mffSearch" parent="toolBar" prefHeight="30px" prefWidth="185px" toolTipText="Поиск"/>
  <Label cursor="0" icon="https://raw.githubusercontent.com/AlexeyKashintsev/Clinic/p5/app/icons/loading5.gif" name="lbWating" parent="view" prefHeight="200px" prefWidth="200px" visible="false">
    <AnchorsPaneConstraints height="200px" left="166px" top="115px" width="200px"/>
  </Label>
  <ModelGrid cursor="0" deletable="false" insertable="false" name="mgServises" parent="view" prefHeight="369px" prefWidth="533px" toolTipText="Список услуг" visible="false">
    <AnchorsPaneConstraints bottom="0px" left="0px" right="0px" top="30px"/>
    <RadioGridColumn minWidth="12px" name="colSelected" preferredWidth="13px" resizable="true" width="13px"/>
    <ModelGridColumn minWidth="105px" name="colName" preferredWidth="110px" readonly="true" title="Имя услуги" width="169px">
      <ModelFormattedField format=""/>
    </ModelGridColumn>
    <ModelGridColumn minWidth="70px" name="colType" preferredWidth="85px" readonly="true" title="Тип" width="143px">
      <ModelFormattedField format=""/>
    </ModelGridColumn>
    <ModelGridColumn maxWidth="100px" minWidth="50px" name="colChekPrepay" preferredWidth="55px" readonly="true" title="Предоплата" width="55px">
      <ModelCheckBox nullable="false"/>
    </ModelGridColumn>
    <ModelGridColumn minWidth="50px" name="colCost" preferredWidth="52px" readonly="true" title="Стоимость" width="111px">
      <ModelFormattedField format=""/>
    </ModelGridColumn>
    <ModelGridColumn maxWidth="75px" minWidth="35px" name="colActive" preferredWidth="40px" readonly="true" title="Активна" width="40px">
      <ModelCheckBox nullable="false"/>
    </ModelGridColumn>
  </ModelGrid>
</layout>
